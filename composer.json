{
  "name": "mindedge/blade",
  "description": "A standalone blade package that implementents a mini IoC container, and binds Illiminate\\view to it",
  "type": "library",
  "authors": [
    {
      "name": "Mindedge Tech Team",
      "email": "tech@mindedge.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Mindedge\\Blade\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "require": {
    "illuminate/container": "5.7.*",
    "illuminate/view": "5.7.*",
    "illuminate/config": "5.7.*",
    "illuminate/database": "5.7.*",
    "illuminate/pagination": "5.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "php-coveralls/php-coveralls": "^2.1",
    "mockery/mockery": "^1.2.0",
    "fzaninotto/faker": "^1.8"
  }
}
